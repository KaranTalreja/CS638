             <script>

              var sscache = {};
              function getPage(url, data, page) {
                var cacheId = url + JSON.stringify(data);
                var cache = sscache;
                var success = function (data) {
                  cache[cacheId] = data;
                  $('#rankContentDiv').html(data['content']);
                  var newpage = page === undefined ? 1 : (parseInt(page) + 1);
                  var pageinfo = "&lt;div align='center' class='pageinfo'&gt;&lt;center&gt; - &lt;/center&gt;&lt;/div&gt;";
                  if (data['max_page'] &gt; 0) {
                    pageinfo = "&lt;div align='center' class='pageinfo'&gt;&lt;center&gt;" + newpage + " of " + data['max_page'] + "&lt;/center&gt;&lt;/div&gt;";
                  }
                  $("#loader").html(pageinfo);

                };

                var error = function (request, errorType) {
                  try {
                    if (page === undefined) {
                      retryButton = '&lt;center&gt;&lt;input id="retryButton" type="button" value="Retry"/&gt;&lt;/center&gt;';
                      $('#rankContentDiv').html(retryButton);
                      $('#retryButton').click(function () {
                        $('#rankContentDiv').html('&lt;p align="center"&gt;&lt;img src="/sites/all/themes/abessive/images/ajax-loader.gif" /&gt; Fetching content&lt;/p&gt;');
                        getPage(url, data, page);
                      });
                    }
                    else {
                      $("#loader").html("&lt;center&gt;Oops! It couldn't load. Try again.&lt;/center&gt;");
                    }
                  }
                  catch (e) {
                  }
                };

                if (cache.hasOwnProperty(cacheId)) {
                  success(cache[cacheId]);
                  return;
                }

                cAjax({
                  url: url,
                  type: "GET",
                  data: data,
                  cache: false,
                  timeout: 10000,
                  dataType: "json",
                  success: success,
                  error: error
                });

              }


              function set_page(element) {
                var page;

                if (element == "prev")
                  page = $("#prev_p").val();
                else if (element == "next")
                  page = $("#next_p").val();

                return page;
              }

              function onload_getpage_ssubmissions(element) {
                $('#loader').html('&lt;p&gt;&lt;img src="/misc/ajax_loader_d.gif" style="width: 120px; height:15px;"/&gt;&lt;/p&gt;');
                var contest = "";
                if ($("#ccode").length != 0) {
                  contest = '&amp;ccode=' + $("#ccode").val();
                }

                var page = set_page(element);
                var data = "page=" + page + "&amp;pcode=" + $("#pcode").val() + contest;
                var url = "/ssubmission/prob";
                getPage(url, data, page);
              }
            </script>
